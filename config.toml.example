[Distributor]
IP = "127.0.0.1"
Port = "19933"
AuthAdapter = "AuthPostgreSQL"

    [Distributor.TLS]
    CertLoc = "/very/complicated/and/long/path/to/your/distributor-cert.pem"
    KeyLoc = "/very/complicated/and/long/path/to/your/distributor-key.pem"

    [Distributor.IMAP]
    Greeting = "Pluto ready."

    [Distributor.AuthFile]
    File = "users.txt"
    Separator = ";"

    [Distributor.AuthPostgreSQL]
    IP = "127.0.0.1"
    Port = "5432"
    Database = "pluto"
    User = "pluto"
    SSLMode = "disable"


[Workers]

    [Workers.worker-1]
    IP = "127.0.0.1"
    Port = "20001"
    # The first ID of the user database table entry
    # that this worker node is responsible for.
    UserStart = 1
    # The last ID of the user database table entry that
    # this worker node is responsible for.
    UserEnd = 10

        [Workers.worker-1.TLS]
        CertLoc = "/very/complicated/and/long/path/to/your/worker-1-cert.pem"
        KeyLoc = "/very/complicated/and/long/path/to/your/worker-1-key.pem"

    [Workers.worker-2]
    IP = "127.0.0.1"
    Port = "20002"
    UserStart = 11
    UserEnd = 20

        [Workers.worker-2.TLS]
        CertLoc = "/very/complicated/and/long/path/to/your/worker-2-cert.pem"
        KeyLoc = "/very/complicated/and/long/path/to/your/worker-2-key.pem"

    [Workers.worker-3]
    IP = "127.0.0.1"
    Port = "20003"
    UserStart = 21
    UserEnd = 30

        [Workers.worker-3.TLS]
        CertLoc = "/very/complicated/and/long/path/to/your/worker-3-cert.pem"
        KeyLoc = "/very/complicated/and/long/path/to/your/worker-3-key.pem"


[Storage]
IP = "127.0.0.1"
Port = "21000"

    [Storage.TLS]
    CertLoc = "/very/complicated/and/long/path/to/your/storage-cert.pem"
    KeyLoc = "/very/complicated/and/long/path/to/your/storage-key.pem"